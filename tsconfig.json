{
	"extends": [
		"@tsconfig/node20/tsconfig.json",
		"@tsconfig/strictest/tsconfig.json"
	],
	"compilerOptions": {
		"module": "CommonJS",
		"moduleResolution": "node10",
		"typeRoots": ["node_modules/@types"],
		"checkJs": false,
		"declaration": true,
		"inlineSourceMap": true,
		"inlineSources": true
	},
	"exclude": [
		"cdk.out",
		"node_modules"
	]
}
